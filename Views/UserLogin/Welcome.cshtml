<!-- jQuery for AJAX request -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        // Function to load sectors via AJAX
        function loadSectors() {
            $.ajax({
                url: '@Url.Action("GetSectors", "Sector")', // URL to fetch sectors
                type: 'GET',
                success: function (data) {
                    // Clear the dropdown before appending new items
                    $('#sectorDropdown').empty();

                    // Add default option
                    $('#sectorDropdown').append('<option value="">-- Select Sector --</option>');

                    // Loop through data and append to dropdown
                    $.each(data, function (index, sector) {
                        $('#sectorDropdown').append('<option value="' + sector.Id + '">' + sector.Name + '</option>');
                    });
                },
                error: function () {
                    alert("An error occurred while loading sectors.");
                }
            });
        }

        // Call the function to load sectors when the page loads
        loadSectors();
    });
</script>
<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            url: '@Url.Action("HandleSectorClick", "Sector")',
            type: 'GET',
            dataType: 'json',
            success: function (response) {
                if (response.success) {
                    var sectorListHtml = "<ul>";
                    $.each(response.data, function (index, sector) {
                        sectorListHtml += "<li>" + sector.SectorCode + " - " + sector.SectorName + "</li>";
                    });
                    sectorListHtml += "</ul>";
                    $('#sector-list').html(sectorListHtml);
                } else {
                    $('#sector-list').html("<p>Error loading sector list: " + response.message + "</p>");
                }
            },
            error: function () {
                $('#sector-list').html("<p>An error occurred while fetching the sector list.</p>");
            }
        });
    });
</script>
